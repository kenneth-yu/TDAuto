{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "clean": "rm -rf .react-router build node_modules/.vite node_modules/.cache tsconfig.tsbuildinfo",
    "build": "node -r ./crypto-polyfill.cjs ./node_modules/@react-router/dev/bin.js build",
    "dev": "node -r ./crypto-polyfill.cjs ./node_modules/@react-router/dev/bin.js dev",
    "start": "react-router-serve build/server/index.js",
    "typecheck": "node -r ./crypto-polyfill.cjs ./node_modules/@react-router/dev/bin.js typegen && tsc -b"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.13.0",
    "tailwindcss": "^4.2.0"
  },
  "devDependencies": {
    "@netlify/vite-plugin-react-router": "^2.1.3",
    "@react-router/dev": "^7.13.0",
    "@react-router/fs-routes": "^7.13.0",
    "@react-router/serve": "^7.13.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "typescript": "^5.6.0",
    "vite-imagetools": "^9.0.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.12"
  }
}
